function hp_d11(g){var b="";b=[];var f="",h=0;for(i=0;i<g.length;i++){c=g.charCodeAt(i);if(c<128)c^=2;f+=String.fromCharCode(c);if(f.length>80){b[h++]=f;f=""}}return b.join("")+f}
